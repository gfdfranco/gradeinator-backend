# .env.development.sample

# Database Configuration
POSTGRES_DB=dev_gradeinator_db
POSTGRES_USER=dev_user
POSTGRES_PASSWORD=dev_password
DATABASE_URL=postgresql://dev_user:dev_password@db:5432/dev_gradeinator_db

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
FLASK_APP=app.py
APP_SECRET_KEY=dev-secret-key-change-me

# Port Configuration
DEV_PORT=5001
PROD_PORT=8000

# AWS Cognito Configuration (Development)
AWS_COGNITO_REGION=your-cognito-region-dev
AWS_COGNITO_USERPOOL_ID=your-cognito-userpool-id-dev
AWS_COGNITO_APP_CLIENT_ID=your-cognito-app-client-id-dev
